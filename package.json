{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "nodemon server/server.ts",
    "client": "webpack serve --mode development",
    "server": "nodemon server/server.ts",
    "server:build": "cd server && tsc && node dist/server.js",
    "server:dev": "cd server && ts-node server.ts",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "dev:colored": "concurrently --kill-others-on-fail -p \"[{name}]\" -n \"SERVER,CLIENT\" -c \"yellow,cyan\" \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "babel": "^5.8.38",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^5.10.9",
    "openai": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^8.2.2",
    "concurrently": "^9.2.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "style-loader": "^2.0.0",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
